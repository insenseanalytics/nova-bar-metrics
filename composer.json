{
    "name": "iaotle/nova-bar-metrics",
    "description": "A Laravel Nova tool for bar chart metrics and frequency distributions.",
    "keywords": [
        "laravel",
        "nova",
        "bar chart",
        "frequency distributions"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Paras Malhotra",
            "email": "paras@insenseanalytics.com"
        }
    ],
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Insenseanalytics\\NovaBarMetrics\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Insenseanalytics\\NovaBarMetrics\\NovaBarMetricsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}